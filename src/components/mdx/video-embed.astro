---
type Props = {
	id: string;
	title: string;
};

const { id, title } = Astro.props;
---

<iframe
	width='100%'
	class='aspect-video'
	src={`https://www.youtube.com/embed/${id}`}
	title={title}
	allow='accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share'
	allowfullscreen></iframe>
